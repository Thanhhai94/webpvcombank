<table style="width: 95%; margin-left: 20px;margin-right: 20px;">
    <tr>
        <td colspan="12" style="background-color: #0772ba; color: white; font-weight: 600;text-align: center; height: 32px;"><%= title %></td>
    </tr>
    <tr style="font-size: 12px; background-color: white;">
        <td  style="width: 70%; padding-left: 10px;height: 28px; "><%= (data.length>0) ? data[0].FULL_NAME : '' %></td>
        <td style="width: 5%; padding-left: 5px;height: 28px; "><i class="fa-solid fa-caret-up green"></i></td>
        <td style="width: 25%; padding-left: 10px;height: 28px; "><%= (data.length>0) ? Math.round(data[0].DIFF).toLocaleString("en-US") : '' %></td>
